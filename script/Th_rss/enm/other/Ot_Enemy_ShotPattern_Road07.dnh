task Road07_ShotPattern03( objID, shote ){
	let swt = 1 + frame % 6 * 10;
	let speed = 1.5;
	let delay = 15;
	let wide = [ 720, 12, 36 ];
	let wt = 30;
	let lp = 0;

	loop( swt ){ yield; }
	
	while( !Obj_IsDeleted( objID ) ){
		loop( 3 ){
			if( Obj_IsDeleted( objID ) ){ return; }
			let p = GetAngleToPlayer( objID );
			let pos = [ ObjMove_GetX( objID ), ObjMove_GetY( objID ) ];
			if( pos[0]>MinX&&pos[0]<MaxX&&pos[1]>MinY&&pos[1]<MaxY-64 ){
				let angle = p - wide[ lp % length( wide ) ] / 2;
				loop( 2 ){
					let sht = CreateShotA2( pos[0], pos[1], 0, angle, speed / 40, speed, shote, delay );
					NotifyEventAll( EV_USER + 40, [ sht, delay ] );
					angle += wide[ lp % length( wide ) ];
				}
				ObjSound_Play( CreateShotSE[ 1 ] );
			}
			lp++;
			loop( wt ){ yield; }
		}
		loop( wt ){ yield; }
	}
}

task Road07_ShotPattern04A( objID, shote ){
	let roundway = 9;
	let wide = 160;
	let delay = 12;
	let wt = 48;
	loop( 100 ){ yield; }
	loop( 12 ){
		if( Obj_IsDeleted( objID ) ){ return; }
		let pos = [ ObjMove_GetX( objID ), ObjMove_GetY( objID ) ];
		let p = GetAngleToPlayer( objID );
		let ag = p - wide / 2;
		loop( roundway ){
			let speed = 3;
			let way = 1;
			let wd = 0;
			loop( 4 ){
				let angle = ag - wd / 2;
				loop( way ){
					let sht = CreateShotA1( pos[0], pos[1], speed, angle, shote, delay );
					NotifyEventAll( EV_USER + 40, [ sht, delay ] );
					angle += wd / ( way - 1 );
				}
				speed /= 1.05;
				wd += 1;
				way++;
			}
			ag += wide / ( roundway - 1 );
		}
		ObjSound_Play( CreateShotSE[ 0 ] );
		loop( wt ){ yield; }
	}
}

task Road07_ShotPattern04B( enmID, objID, shote ){
	let lp = 12;
	let delay = 15;
	let wide = 16;
	let way = 4;
	let wtA = 2;
	let wtB = 30;
	let r = 1;
	loop( 140 ){ yield; }
	loop( 7 ){
		let speed = 2.2;
		let p = atan2( ObjMove_GetY( objID ) - ObjMove_GetY( enmID ), ObjMove_GetX( objID ) - ObjMove_GetX( enmID ) ) + 150 * r;
		loop( lp ){
			if( Obj_IsDeleted( objID ) ){ return; }
			let pos = [ ObjMove_GetX( objID ), ObjMove_GetY( objID ) ];
			let angle = p - wide / 2 + round( rand( -1, 1 ) ) * 0.4;
			loop( way ){
				let sht = CreateShotA2( pos[0], pos[1], 0, angle, speed / 30, speed, shote, delay );
				NotifyEventAll( EV_USER + 40, [ sht, delay ] );
				angle += wide / ( way - 1 );
			}
			ObjSound_Play( CreateShotSE[ 0 ] );
			speed += 0.07;
			loop( wtA ){ yield; }
		}
		r = -r;
		loop( wtB ){ yield; }
	}
}


task Road07_ShotPattern06( objID, shote ){
	let swt = 100;
	let delay = 15;
	let roundway = 3;
	let wide = [ 720, 10 ];
	let wt = 60;
	let lp = 0;

	loop( swt ){ yield; }
	
	loop( 4 ){
		let speed = 2;
		let lp = 0;
		loop( 3 ){
			if( Obj_IsDeleted( objID ) ){ return; }
			let p = GetAngleToPlayer( objID );
			let pos = [ ObjMove_GetX( objID ), ObjMove_GetY( objID ) ];
			let ag = p;
			loop( roundway ){
				let angle = ag - wide[ lp % length( wide ) ] / 2;
				loop( 2 ){
					let sht = CreateShotA2( pos[0], pos[1], 0, angle, speed / 40, speed, shote, delay );
					NotifyEventAll( EV_USER + 40, [ sht, delay ] );
					angle += wide[ lp % length( wide ) ];
				}
				ag += 360 / roundway;
			}
			ObjSound_Play( CreateShotSE[ 1 ] );
			lp++;
			speed /= 1.15;
		}
		loop( wt ){ yield; }
	}
}


task Road07_ShotPattern07A( objID, shote ){
	let swt = 120;
	let roundway = 9;
	let speed = 3;
	let delay = 15;
	let wtA = 2;
	let wtB = 50;
	loop( swt ){ yield; }
	
	loop( 5 ){
		let p = GetAngleToPlayer( objID );
		let wide = 720;
		loop( 7 ){
			if( Obj_IsDeleted( objID ) ){ return; }
			let pos = [ ObjMove_GetX( objID ), ObjMove_GetY( objID ) ];
			let ag = p;
			loop( roundway ){
				let angle = ag - wide / 2;
				loop( 2 ){
					let sht = CreateShotA2( pos[0], pos[1], 0, angle, speed / 40, speed, shote, delay );
					NotifyEventAll( EV_USER + 40, [ sht, delay ] );
